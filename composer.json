{
    "name": "bissolli/validador-cpf-cnpj",
    "description": "Classe em PHP para validação de CPF e CNPJ.",
    "keywords": [
        "cnpj",
        "cpf",
        "validador",
        "documento",
        "brasil",
        "laravel"
    ],
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/bissolli/validador-cpf-cnpj",
    "authors": [
        {
            "name": "Gustavo Bissolli",
            "email": "gustavo.bissolli@gmail.com"
        },
        {
            "name": "Lucas Ferro",
            "email": "lucasferrobrandao@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Bissolli\\ValidadorCpfCnpj\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bissolli\\ValidadorCpfCnpj\\Test\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "test": [
            "vendor/bin/phpunit"
        ]
    }
}
